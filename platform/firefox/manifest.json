{
  "author": "Daniel C. Howe",
  "developer": {
    "name": "Daniel C. Howe",
    "url": "https://rednoise.org/daniel"
  },
  "background": {
    "page": "background.html"
  },
  "browser_action": {
    "browser_style": false,
    "default_area": "navbar",
    "default_icon": {
      "16": "img/icon_16.png",
      "32": "img/icon_32.png",
      "64": "img/icon_64.png"
    },
    "default_title": "AdNauseam",
    "default_popup": "menu.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "adnauseam@rednoise.org",
      "strict_min_version": "92.0"
      "data_collection_permissions": {
        "required": [ "none" ]
      }
    },
    "gecko_android": {
      "strict_min_version": "92.0"
    }
  },
  "commands": {
    "_execute_browser_action": {},
    "launch-element-zapper": {
      "description": "__MSG_popupTipZapper__"
    },
    "launch-element-picker": {
      "description": "__MSG_popupTipPicker__"
    },
    "launch-logger": {
      "description": "__MSG_popupTipLog__"
    },
    "open-dashboard": {
      "description": "__MSG_popupTipDashboard__"
    },
    "relax-blocking-mode": {
      "description": "__MSG_relaxBlockingMode__"
    },
    "toggle-cosmetic-filtering": {
      "description": "__MSG_toggleCosmeticFiltering__"
    },
    "toggle-javascript": {
      "description": "__MSG_toggleJavascript__"
    }
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*",
        "file://*/*"
      ],
      "js": [
        "/js/vapi.js",
        "/js/vapi-client.js",
        "/js/contentscript.js",
        "js/adn/textads.js",
        "js/adn/parser.js"
      ],
      "all_frames": true,
      "match_about_blank": true,
      "run_at": "document_start"
    },
    {
      "matches": [
        "https://easylist.to/*",
        "https://*.fanboy.co.nz/*",
        "https://filterlists.com/*",
        "https://forums.lanik.us/*",
        "https://github.com/*",
        "https://*.github.io/*"
      ],
      "js": [
        "/js/scriptlets/subscriber.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    },
    {
      "matches": [
        "https://github.com/uBlockOrigin/*",
        "https://ublockorigin.github.io/*",
        "https://*.reddit.com/r/uBlockOrigin/*"
      ],
      "js": [
        "/js/scriptlets/updater.js"
      ],
      "run_at": "document_idle",
      "all_frames": false
    }
  ],
  "default_locale": "en",
  "description": "__MSG_extShortDesc__",
  "icons": {
    "48": "img/icon_48.png",
    "96": "img/icon_96.png"
  },
  "manifest_version": 2,
  "name": "AdNauseam",
  "options_ui": {
    "page": "dashboard.html",
    "open_in_tab": true
  },
  "permissions": [
    "alarms",
    "dns",
    "menus",
    "privacy",
    "storage",
    "tabs",
    "unlimitedStorage",
    "webNavigation",
    "webRequest",
    "webRequestBlocking",
    "management",
    "<all_urls>"
  ],
  "short_name": "AdNauseam",
  "sidebar_action": {
    "default_title": "__MSG_statsPageName__",
    "default_panel": "logger-ui.html",
    "default_icon": "img/ublock.svg",
    "open_at_install": false
  },
  "version": "{version}",
  "web_accessible_resources": [
    "/web_accessible_resources/*"
  ]
}